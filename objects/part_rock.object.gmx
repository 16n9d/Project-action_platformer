        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Particle logic
// Make the particle bounce on the ground
if (place_meeting(x, y + 1, par_fake)) {
    // Check if it is able to hup
    if (hups &gt; 0) {
        jspd -= (jspd * 0.25);
        vspd = jspd; 
        hups--;
    } else {
        vspd = 0;
    }
    // Slow down the motion speed
    mspd += (0 - mspd) * 0.05;
} else {
    // Add gravity
    if (vspd &lt; 7) {
        vspd += grav;
    }
}

// BURN!! BURN!!!!
if (lit &amp;&amp; image_xscale &gt; 0.80) {
    var i = instance_create(x, y, part_fire);
        i.image_xscale *= 0.75;
        i.image_yscale *= 0.75;
    if (light == noone) {
        light = scr_ls_light_create(x, y, 32, c_orange, 0.05, id);    
    }
} else {
    with(light) instance_destroy();
}

// Stop the particle of moving horizontally if collision with a solid
if (place_meeting(x + hspd, y, par_fake)) {
    mspd = 0;
}

hspd = mspd;

// If some time has pass destroy the particle with style
if (hspd == 0 &amp;&amp; vspd == 0) {
    image_xscale += (0 - image_xscale) * 0.01;
    image_yscale = image_xscale;
}

if (image_xscale &lt;= 0) instance_destroy();

// Particle built in cfg
x += hspd;

// Vertical Collisions
if (place_meeting(x , y + vspd, par_fake)) {
    vspd = 0;
}

y += vspd;

</string>
